{
  "name": "open-fans",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "node ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "dev:api-only": "bun --watch server/index.ts",
    "start:api-only": "bun server/index.ts",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "db:generate": "drizzle-kit generate",
    "db:push": "mkdir -p tmp && drizzle-kit push --force",
    "db:studio": "drizzle-kit studio",
    "db:reset": "rm -rf ./tmp && mkdir -p tmp && drizzle-kit push --force"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.3.5",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-router/fs-routes": "^7.7.1",
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.3",
    "drizzle-zod": "^0.8.2",
    "hono": "^4.8.9",
    "isbot": "^5.1.27",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.526.0",
    "nanoid": "^5.1.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.1",
    "react-router-hono-server": "^2.16.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.2.0",
    "drizzle-kit": "^0.31.4",
    "esbuild": "^0.20.1",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
